  "version": "0.2.0",
  "configurations": [
    {
      "type": "chrome",
      "request": "launch",
      "name": "Launch Localhost",
      "url": "http://localhost:3000",
      "webRoot": "${workspaceRoot}/public",
      "sourceMaps": true,
      "sourceMapPathOverrides": {
        "webpack:///*": "${webRoot}/../*"
      }
